{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES5",
    "sourceMap": true,
    "outDir": "./dist",
    "incremental": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "strict": false,
    "noImplicitAny": false,
    "strictNullChecks": true,
    "strictFunctionTypes": false,
    "strictPropertyInitialization": true,
    "strictBindCallApply": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "alwaysStrict": true,
    "pretty": true,
    "disableSizeLimit": true,
    "baseUrl": "./",
    "paths": {
      "@app/kafka-topics": ["libs/kafka-topics/src"],
      "@app/kafka-topics/*": ["libs/kafka-topics/src/*"],
      "@app/fastify-adapter-configure": ["libs/fastify-adapter-configure/src"],
      "@app/fastify-adapter-configure/*": [
        "libs/fastify-adapter-configure/src/*"
      ],
      "@app/provider-generation-functions": [
        "libs/provider-generation-functions/src"
      ],
      "@app/provider-generation-functions/*": [
        "libs/provider-generation-functions/src/*"
      ]
    }
  }
}
